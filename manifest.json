{
    "manifest_version": 3,
    "name": "Hide Pink Extension",
    "version": "1.0",
    "description": "An extension to hide pink elements",
    "permissions": ["scripting", "activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*.teleopticloud.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/pink_square_red_x_16px.png",
        "48": "icons/pink_square_red_x_48px.png",
        "128": "icons/pink_square_red_x_128px.png"
      }
    },
    "icons": {
      "16": "icons/pink_square_red_x_16px.png",
      "48": "icons/pink_square_red_x_48px.png",
      "128": "icons/pink_square_red_x_128px.png"
    }
  }
  